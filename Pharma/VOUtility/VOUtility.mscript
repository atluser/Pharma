String getContactName(String contactId){
		Query q=vObjContact.getContactRow(contactId);
		DataRow row=q.getRowSet().first();
		String contactName="";
		if(row!=null)
			contactName=vObjContact.ContactName.getStringValue(row);
		
		return contactName;		
	}
	
}


