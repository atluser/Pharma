TableViewPanel {
	Label lbl=new Label(); 
	callback onCreate(Container c){
		Font fnt = new Font();
		fnt.setSize(18);
		fnt.setFamily("Arial");
		lbl.setFont(fnt);
		lbl.setForeColor(Color.create(255,255,255));
		add(lbl,"halign=center,valign=center,hfill=fill");
		setBackground(Background.create9PartImageRectangleBackground(Images.VodafoneImages.img_actionbar_bg, 0, 0, 0, 0));
	}

}
